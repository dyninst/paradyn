TARGET	      = barChart

IFLAGS	     += -I../../../paradyn/src/UIthread \
		-I../../../util/h

VPATH	     += ../../../paradyn/src/UIthread

SRCS	     += ../../../paradyn/src/UIthread/tkTools.C \
		../../../paradyn/src/UIthread/pdLogo.C \
		../../../paradyn/src/UIthread/minmax.C \
		../barChart.C \
		../barChartTcl.C \
		../barChartDriver.C \
		../dg2.C \
		../templates.C

TCL_SRCS	= ../barChart.tcl
#TCL_SRCS	= 
# $(wildcard ../*.tcl)
SRCS		+= $(TCL2C_GEN_SRCS)

LIBS         += -lvisi -lpdutil 
SYSLIBS	     += -ltk -ltcl -lX11 -lm

all:	$(TARGET)

$(TCL2C_GEN_SRCS): $(TCL_SRCS)
	$(TCL2C) $^ > $@
