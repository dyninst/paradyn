TARGET	      = tclVisi

SRCS	     += ../src/tclHack.c \
		../src/tclVisi.C \
		../src/templates.C \
		../src/DGclient.C

LIBS         += -lvisi -lhist -lutil 
SYSLIBS	     += -ltk -ltcl -lXaw -lXext -lXmu -lXt -lX11 -lm


all: $(TARGET)

templates.o: ../src/templates.C
	$(CXX) $(TFLAGS) -c ../src/templates.C
