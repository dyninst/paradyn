TARGET2	      = libpdutilPVM.a

SRCS	     += ../src/String.C \
		../src/DictionaryLite.C \
		../src/lprintf.C \
		../src/rpcUtil.C \
		../src/aggregateSample.C \
		../src/stringPool.C \
		../src/hist.C \
		../src/timing.C \
		../src/Timer.C \
		../src/makenan.C \
		../src/PriorityQueue.C \
