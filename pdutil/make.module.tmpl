TARGET	      = libutil.a
TARGET2	      = libutilPVM.a

SRCS	     += ../src/lprintf.C \
		../src/rpcUtil.C \
		../src/tunableConst.C \
		../src/aggregateSample.C \
		../src/stringPool.C \
		../src/hist.C \
		../src/timing.C 

OBJS         += lprintf.o rpcUtil.o tunableConst.o aggregateSample.o \
 		stringPool.o hist.o timing.o

SRCS2	     += rpcUtilPVM.C
OBJS2	     += rpcUtilPVM.o

all: $(TARGET) $(TARGET2) 

TFLAGS +=  $(USEFULWARNINGS) -DDO_INLINE_F="inline"
CXXFLAGS = $(TFLAGS) -g
