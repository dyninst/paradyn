TARGET	      = igen

SRCS 	     += ../src/main.C \
		parser.C \
		scanner.C \
		../src/interface_spec.C \
	        ../src/templates.C 

LIBS 	     += -lpdutil -lfl
CXXFLAGS       += $(USEFULWARNINGS) 

all:	$(TARGET)

templates.o: ../src/templates.C 
	$(CXX) -DDO_INLINE="inline" $(TFLAGS) $(USEFULWARNINGS) -c ../src/templates.C
