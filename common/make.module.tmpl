TARGET	      = libpdutil.a
TARGET2	      = libpdutilPVM.a

SRCS	     += ../src/String.C \
		../src/lprintf.C \
		../src/rpcUtil.C \
		../src/aggregateSample.C \
		../src/stringPool.C \
		../src/hist.C \
		../src/timing.C 

OBJS         += String.o lprintf.o rpcUtil.o aggregateSample.o \
 		stringPool.o hist.o timing.o

SRCS2	     += rpcUtilPVM.C
OBJS2	     += rpcUtilPVM.o

all: $(TARGET) $(TARGET2) 
