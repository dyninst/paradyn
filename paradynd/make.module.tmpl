TARGET	      = paradynd

VPATH	     += ../../paradynd/src:../../paradyn/h

SRCS	     += ../src/association.C \
		../src/ast.C \
		../src/comm.C \
		../src/context.C \
		../src/debugger.C \
		../src/dynrpc.C \
		../src/init.C \
		../src/inst-sparc.C \
		../src/inst-sunos.C \
		../src/inst.C \
		../src/main.C \
		../src/metric.C \
		../src/perfStream-def.C \
		../src/perfStream.C \
		../src/primitives.C \
		../src/process.C \
		../src/resource.C \
		../src/stats.C \
		../src/symtab.C \
		../src/templates.C \
		../src/util.C \
 		../src/mdl.C


IGEN_GEN_SRCS = dyninstRPC.xdr.SRVR.C \
		dyninstRPC.xdr.C

IGEN_ISRCS    = dyninstRPC.I

LIBS	     += -lpdutil -liberty

IFLAGS       += -I../$(PLATFORM)
CXXFLAGS     += $(USEFULWARNINGS) 

all:    $(TARGET)

templates.o: ../src/templates.C
	$(CXX) $(TFLAGS) -c ../src/templates.C

