TARGET	      = libvisi.a 

VPATH        += ../../visi/h
VPATH        += ../../util/h

SRCS	     += ../src/templates.C \
		../src/datagrid.C \
		../src/visualization.C 


IGEN_GEN_SRCS =	visi.xdr.SRVR.C \
		visi.xdr.CLNT.C \
		visi.xdr.C

IGEN_ISRCS    = visi.I

all: $(TARGET)

templates.o: ../src/templates.C
	$(CXX) $(TFLAGS) -c ../src/templates.C
